<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="@string/app_name"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:background="#9f9a9a"
        android:id="@+id/textView" />
    <ImageButton
        android:id="@+id/imageButton31"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/imageButton32"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="33dp"
        android:layout_marginStart="33dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/qianjin" />

    <ImageButton
        android:id="@+id/imageButton32"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/imageButton31"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageButton31"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/houtui" />

    <ImageButton
        android:id="@+id/imageButton33"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageButton34"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="10dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/youzhuan" />

    <ImageButton
        android:id="@+id/imageButton34"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/textView35"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/imageButton33"
        android:layout_toStartOf="@+id/imageButton33"

        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/zuozhuan" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton31"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="35dp"
        android:layout_toEndOf="@+id/imageButton32"
        android:layout_toRightOf="@+id/imageButton32"
        android:text="车轮速度" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/seekBar"
        android:layout_alignTop="@+id/seekBar"
        android:layout_marginLeft="10dp"
        android:text="0" />

    <SeekBar
        android:id="@+id/seekBar"
        style="@android:style/Widget.SeekBar"
        android:layout_width="250dp"
        android:layout_height="wrap_content"

        android:layout_alignTop="@+id/imageButton31"
        android:layout_toEndOf="@+id/imageView"
        android:layout_toRightOf="@+id/imageView"
        android:max="14"
        android:progress="0" />

    <ListView
        android:id="@+id/ListView"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textViewZhuangtai"
        android:layout_marginTop="8dp"
        />

    <SeekBar
        android:id="@+id/seekBarClean"
        style="@android:style/Widget.SeekBar"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/seekBar"
        android:layout_alignStart="@+id/seekBar"
        android:layout_below="@+id/imageButton34"
        android:layout_marginBottom="6dp"
        android:max="14"
        android:progress="0" />

    <TextView
        android:id="@+id/textViewCleanSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView32"
        android:layout_alignStart="@+id/textView32"
        android:layout_alignTop="@+id/seekBarClean"
        android:text="风扇速度" />

    <TextView
        android:id="@+id/textViewClean"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView35"
        android:layout_alignRight="@+id/textView35"
        android:layout_alignTop="@+id/seekBarClean"
        android:text="0" />

    <TextView
        android:id="@+id/textViewZhuangtai"
        android:layout_width="150dp"
        android:layout_height="180dp"
        android:layout_below="@+id/textView"
        android:layout_marginTop="2dp"
        android:layout_centerHorizontal="true"
        android:gravity="center_vertical"
        android:text=""
        android:textColor="#FF3BEF2E" />

    <ImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignBottom="@+id/textViewZhuangtai"
        android:layout_alignLeft="@+id/imageButton31"
        android:layout_alignStart="@+id/imageButton31"
        android:layout_marginBottom="21dp"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:background="@drawable/car"
        android:id="@+id/imageView" />


</RelativeLayout>